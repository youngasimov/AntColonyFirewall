package antcolonyfirewall.core.implemented;

/**
 *
 * @author camilovera
 */
public class DefaultPolicyRule extends Rule {
    
    public DefaultPolicyRule(String defaultPolicy){
        super(-1);
        super.setSourceAddress(Rule.ANY);
        super.setSourcePort(Rule.ANY);
        super.setDestinationAddress(Rule.ANY);
        super.setDestinationPort(Rule.ANY);
        super.setAction(defaultPolicy);
    }
    
}
